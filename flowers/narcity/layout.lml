<!DOCTYPE html>
{#config;theme;extra;debug}
<html id="lmlhtml" data-htmltype="lml" data-generated="true" data-cachehit="true" lang="{=extra.language}">
<head id="lmlhead">
    {%petals/meta}
    {%petals/headstyle}
    {%petals/headscript}
    {=theme.settings.headprio}
    {=theme.getQueueTags()}
    {%petals/title}
    {=theme.settings.headend}
</head>
<body class="context-{=extra.contextname} narcity-theme" data-device="">
    {=theme.settings.bodyprio}
    {%petals/bodyopening}
    {%petals/socialhead}
    {%petals/mobileslider}

    {%petals/header}
    {%petals/sharer}

    <div id="mcanvas">
        <div id="container">
            {%petals/beforecontent}
            {=extra.contentHTML}

            <div id="lmlasync">

            </div>
        </div>
    </div>
    {%petals/footer}

    {=theme.settings.bodyend}
</body>
</html>
