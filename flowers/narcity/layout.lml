<!DOCTYPE html>
{#config;theme;extra;debug}
<html id="lmlhtml" data-htmltype="lml" data-generated="true" data-cachehit="true" lang="{=config.default.website.language}">
<head id="lmlhead">
    {=theme.settings.headprio}
    {%petals/meta}
    {%petals/headstyle}
    {%petals/headscript}
    {=theme.getQueueTags()}
    {%petals/title}
    {=theme.settings.headend}
</head>
<body>
    {=theme.settings.bodyprio}
    {%petals/lib}
    {%petals/socialhead}
    {%petals/mobileslider}

    <div id="mcanvas">
        {%petals/header}
        {%petals/beforecontent}
        {=extra.contentHTML}
        {%petals/footer}
    </div>

    {=theme.settings.bodyend}
</body>
</html>
