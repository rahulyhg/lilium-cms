var parseEmbeds = function() {
    var parags = document.querySelectorAll(".article-content p"); 
    for (var i = 0; i < parags.length; i++) { 
        var p = parags[i]; 
        var txt = p.textContent; 

        if (txt.indexOf("https://youtu") != -1) {
            var splt = txt.split('/');
            var ytid = splt.pop() || splt.pop();
            p.innerHTML = '<iframe class="lml-youtube" src="https://www.youtube.com/embed/'+ytid+'" frameborder="0" allowfullscreen></iframe>';
        } 
    }
}

var parseContentLinks = function() {
    var links = document.querySelectorAll('#article-text a');
    for (var i = 0; i < links.length; i++) {
        links[i].target = "_blank";
    }
};

var presentTrending = function() {
    var wrap = document.querySelectorAll('.trending-wrapper');
    for (var i = 0; i < wrap.length; i++) {
        var generator = wrap[i].dataset.generator;
        if (generator && window.generators && window.generators[generator]) {
            window.generators[generator].apply(wrap[i], window.lmltrending);
        }
    }
};

var rearrangeImages = function() {
    var asis = document.querySelectorAll('.image-asis');
    for (var i = 0; i < asis.length; i++) {
        var w = asis[0].getAttribute('width');
        asis[i].style.width = w + "px";
        asis[i].style.display = "inline-block";
        asis[i].style.verticalAlign = "middle";
    }
};

