        transition: all 0.4s;

    -webkit-transform: translate3d(0, 100%, 0);
   -moz-transform: translate3d(0, 100%, 0);
    -ms-transform: translate3d(0, 100%, 0);
     -o-transform: translate3d(0, 100%, 0);
        transform: translate3d(0, 100%, 0);

    opacity: 0;
}

#android-sharer-buttons.shown {
    -webkit-transform: translate3d(0,0,0);
   -moz-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
     -o-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);

    opacity: 1;
}

#android-sharer-buttons h3 {
    margin-left: 20px;
}

#android-sharer-buttons ul {
    list-style: none;
    padding:0px;
    margin:0px;
    float: none;
    clear:both;
}

#android-sharer-buttons ul li {
    float: left;
    width: 33.33%;
    max-width: 200px;
    text-align: center;
    height: 50px;
    padding: 6% 0px;
}

#android-sharer-buttons ul li i {
    font-size: 32px;
    display: block;
    margin: 0 auto 6px;
}

#android-sharer-cancel {
    display: block;
    text-align: right;
    color: #ff2634;
    float: none;
    clear:both;
    padding: 20px;
}

body.slidheader .android-sharer {
    -webkit-transform: translate3d(0,0,0);
   -moz-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
     -o-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);

    opacity: 1;
}

.android-sharer i{
    margin: 16px;
}

#pc-sharer i {
    margin: 16px 7px 16px 18px;
    vertical-align: middle;
}

#pc-sharer span {
    vertical-align: middle;
}

.android .android-sharer {
    display: block;
}

.ios-sharer {
    display: none;
}

.ios .ios-sharer {
    display: block;
    height: 50px;

    -webkit-transform: translate3d(0, 50px, 0);
   -moz-transform: translate3d(0, 50px, 0);
    -ms-transform: translate3d(0, 50px, 0);
     -o-transform: translate3d(0, 50px, 0);
        transform: translate3d(0, 50px, 0);

    -webkit-transition: all 0.2s;
   -moz-transition: all 0.2s;
    -ms-transition: all 0.2s;
     -o-transition: all 0.2s;
        transition: all 0.2s;

    background-color: #FFF;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 500;
    color: #FFF;
    font-size: 20px;

    font-family: "Oswald", sans-serif;
}

#ios-sharer i {
