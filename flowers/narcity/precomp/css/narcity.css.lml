#pc-sharer {
    display: none;

    z-index: 30;
    position: fixed;
    bottom: 40px;
    right: 40px;
    padding: 0px;
    background-color: {=mc};
    border-radius: 50px;
    height: 60px;
    width: 60px;
    color: #FFF;
    font-size: 28px;
    box-shadow: 0px 0px 12px rgba(0,0,0,0.1);
    font-family: Arial;
    font-weight: bold;

    -webkit-transition: all 0.2s;
   -moz-transition: all 0.2s;
    -ms-transition: all 0.2s;
     -o-transition: all 0.2s;
        transition: all 0.2s;

    opacity: 0;
    cursor: pointer;

    -webkit-transform: translate3d(0px, 160px, 0);
   -moz-transform: translate3d(0px, 160px, 0);
    -ms-transform: translate3d(0px, 160px, 0);
     -o-transform: translate3d(0px, 160px, 0);
        transform: translate3d(0px, 160px, 0);

    overflow: hidden;
    white-space: nowrap;
}

.pc-sharer-network {
    display: none;
    z-index: 30;
    position: fixed;
    bottom: 40px;
    right: 40px;
    padding: 0px;
    background-color: #999;
    border-radius: 50px;
    height: 60px;
    width: 60px;
    color: #FFF;
    font-size: 28px;
    box-shadow: 0px 0px 12px rgba(0,0,0,0.1);

    -webkit-transition: all 0.2s;
   -moz-transition: all 0.2s;
    -ms-transition: all 0.2s;
     -o-transition: all 0.2s;
        transition: all 0.2s;

    opacity: 0;
    cursor: pointer;

    -webkit-transform: translate3d(0px, 160px, 0);
   -moz-transform: translate3d(0px, 160px, 0);
    -ms-transform: translate3d(0px, 160px, 0);
     -o-transform: translate3d(0px, 160px, 0);
        transform: translate3d(0px, 160px, 0);
}

#upnext-wrapper {
    position: fixed;
    bottom: -100%;
    left: 0;
    right: 0;
    height: 100px;
    background-color: #FFF;
    z-index: 100;
    text-align: center;
    font-family: "Oswald", sans-serif;
    font-weight: normal;
    color: #333;

    cursor: grab;
    border-top: 2px solid #DDD;

    -webkit-transition: bottom 0.2s;
       -moz-transition: bottom 0.2s;
        -ms-transition: bottom 0.2s;
         -o-transition: bottom 0.2s;
            transition: bottom 0.2s;

      -webkit-touch-callout: none; /* iOS Safari */
    -webkit-user-select: none; /* Safari */
     -khtml-user-select: none; /* Konqueror HTML */
       -moz-user-select: none; /* Firefox */
        -ms-user-select: none; /* Internet Explorer/Edge */
            user-select: none; /* Non-prefixed version, currently
                                  supported by Chrome and Opera */
}

#upnext-wrapper.slid {
    bottom: 0;
}

#upnext-wrapper:hover, #upnext-wrapper:active {
    border-top: 2px solid {=mc};
}

#upnext-blank {
    position: fixed;
    top:0; bottom:0; left:0; right:0;
    z-index: 120;

    -webkit-transform: translate3d(0px, 100%, 0px);
       -moz-transform: translate3d(0px, 100%, 0px);
        -ms-transform: translate3d(0px, 100%, 0px);
         -o-transform: translate3d(0px, 100%, 0px);
            transform: translate3d(0px, 100%, 0px);

    background: #fff;
}

#upnext-wrapper.trans,
#upnext-blank.trans {
    -webkit-transition: all 0.4s;
       -moz-transition: all 0.4s;
        -ms-transition: all 0.4s;
         -o-transition: all 0.4s;
            transition: all 0.4s;
}

#upnext-wrapper.thrown {
    -webkit-transform: translate3d(0px, -100%, 0px);
       -moz-transform: translate3d(0px, -100%, 0px);
        -ms-transform: translate3d(0px, -100%, 0px);
         -o-transform: translate3d(0px, -100%, 0px);
            transform: translate3d(0px, -100%, 0px);
}

#upnext-blank.thrown {
    -webkit-transform: translate3d(0px, 0%, 0px);
       -moz-transform: translate3d(0px, 0%, 0px);
        -ms-transform: translate3d(0px, 0%, 0px);
         -o-transform: translate3d(0px, 0%, 0px);
            transform: translate3d(0px, 0%, 0px);
}

#upnext-wrapper h3 {
    font-weight: normal;
    margin: 10px 0px;
    text-transform: uppercase;
    cursor : pointer;
}

#upnext-wrapper h3 i {
    font-size: 25px;
    margin: 0px 6px;
    color: #AAA;
    cursor : default;
}

#upnext-wrapper h4 {
    font-weight: normal;
    margin-top: 0;
    cursor : default;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0px 20px;
    display: block;
    overflow: hidden;
}

