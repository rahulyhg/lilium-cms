{#config;theme}
{$
    siteurl = config.default.server.url;
    fw = theme.settings.fullwidthsize;
    fw += "px";

    aw = theme.settings.articlewidthsize;
    aw += "px";

    mc = theme.settings.maincolor;

    awpad = theme.settings.articlewidthsize;
    awpad -= 40;
    awpad += "px";
$}

@import url('https://fonts.googleapis.com/css?family=PT+Serif:400,400i,700');

@font-face {
    font-family: 'Oswald';
    src: url('{=siteurl}/static/fonts/Oswald/oswald-regular.eot');
    src: url('{=siteurl}/static/fonts/Oswald/oswald-regular.eot?#iefix') format('embedded-opentype'),
         url('{=siteurl}/static/fonts/Oswald/oswald-regular.woff2') format('woff2'),
         url('{=siteurl}/static/fonts/Oswald/oswald-regular.woff') format('woff'),
         url('{=siteurl}/static/fonts/Oswald/oswald-regular.ttf') format('truetype'),
         url('{=siteurl}/static/fonts/Oswald/oswald-regular.svg#oswaldbook') format('svg');
    font-weight: normal;
    font-style: normal;
}

html, body {
    font-family: 'PT Serif', serif;
    color: #111;

    padding: 0px;
    margin: 0px;
    opacity : 1;
}

body.locked {
    overflow: hidden;
}

body.opacitytrans {
    -webkit-transition: opacity 0.4s;
       -moz-transition: opacity 0.4s;
        -ms-transition: opacity 0.4s;
         -o-transition: opacity 0.4s;
            transition: opacity 0.4s;
}

body.willfadein {
    opacity : 0;
}

#mcanvas {
    margin-top: 60px;

    position: relative;
    -webkit-transition: all 0.2s;
   -moz-transition: all 0.2s;
    -ms-transition: all 0.2s;
     -o-transition: all 0.2s;
        transition: all 0.2s;

    -webkit-transform: none;  
       -moz-transform: none; 
        -ms-transform: none; 
         -o-transform: none; 
            transform: none; 
}

#mcanvasoverlay {
    position: fixed;
    top: 0; bottom:0; left:0; right:0;
    background-color: #000;
    opacity: 0;

    display: none;

    -webkit-transform: translate3d(0,0,0);
    -moz-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
    -o-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);

    -webkit-transition: opacity 0.2s;
   -moz-transition: opacity 0.2s;
    -ms-transition: opacity 0.2s;
     -o-transition: opacity 0.2s;
        transition: opacity 0.2s;

    z-index: 500;
}

.nsfw #mcanvas {
    margin-top: 80px;
}

#lmlasync {
    float: none;
    display: block;
    clear: both;
}

body.menuopen #mcanvasoverlay,
#mcanvasoverlay.shown {
    opacity: 0.5;
}

body.menuopen #mcanvas {
    -webkit-transform: translate3d(100px, 0px, 0px);
   -moz-transform: translate3d(100px, 0px, 0px);
    -ms-transform: translate3d(100px, 0px, 0px);
     -o-transform: translate3d(100px, 0px, 0px);
        transform: translate3d(100px, 0px, 0px);
}

#mobileslider {
    position: fixed;
    left:0;
    top:0;
    bottom:0;
    width: 250px;

    z-index: 1000;
    overflow-y: auto;

    background-color: #FFF;

    -webkit-transform: translate3d(-280px, 0, 0);
   -moz-transform: translate3d(-280px, 0, 0);
    -ms-transform: translate3d(-280px, 0, 0);
     -o-transform: translate3d(-280px, 0, 0);
        transform: translate3d(-280px, 0, 0);

    -webkit-transition: all 0.2s;
   -moz-transition: all 0.2s;
    -ms-transition: all 0.2s;
     -o-transition: all 0.2s;
        transition: all 0.2s;

    opacity: 0;

    padding-bottom: 15px;
}

body.menuopen {
    overflow: hidden;
}

body.menuopen #mobileslider {
    -webkit-transform: translate3d(0,0,0);
   -moz-transform: translate3d(0,0,0);
    -ms-transform: translate3d(0,0,0);
     -o-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);

    opacity: 1;
}

#container .narcity-header-bar {
    position: relative;
    top: 0;
    left: 0;
    right: 0;
}

#mcanvas {
    position: relative;
    z-index: 2;
}

.lbtop {
    z-index: 5;
    position: relative;
    text-align: center;
}

.lbtop.willslide {
    padding: 20px 0px 15px;
    text-align: center;
    position: relative;
}

.midvert {
    display: inline-block;
    vertical-align: middle;
}

.float-clear {
    clear: both;
    float: none;
}

.topvert {
    display: inline-block;
    vertical-align: top;
}

html .invisible {
    display: none;
}

.narcity-header-bar {
    height: 60px;
    background-color: {=theme.settings.headercolor};

    font-family: "Oswald", Arial, sans-serif;
    font-size: 25px;
    text-transform: uppercase;
    color: #FFF;

    position: fixed;
    top:0; left:0; right:0;

    z-index : 15;
    opacity: 1;

    -webkit-transition: all 0.2s;
       -moz-transition: all 0.2s;
        -ms-transition: all 0.2s;
         -o-transition: all 0.2s;
            transition: all 0.2s;
}

body.slidheader .narcity-header-bar {
    -webkit-transform: translate3d(0, -80px, 0);
       -moz-transform: translate3d(0, -80px, 0);
        -ms-transform: translate3d(0, -80px, 0);
         -o-transform: translate3d(0, -80px, 0);
            transform: translate3d(0, -80px, 0);

    opacity: 0;
}

.willslide {
    -webkit-transition: all 0.2s;
       -moz-transition: all 0.2s;
        -ms-transition: all 0.2s;
         -o-transition: all 0.2s;
            transition: all 0.2s;
}

body.menuopen .willslide,
body.menuopen .narcity-header-bar {
    -webkit-transform: translate3d(100px,0,0);
    -moz-transform: translate3d(100px,0,0);
    -ms-transform: translate3d(100px,0,0);
    -o-transform: translate3d(100px,0,0);
    transform: translate3d(100px,0,0);
}

.header-menu-bar-wrapper {
    display: inline-block;
    height: 26px;
    padding: 17px;
    vertical-align: middle;
    cursor: pointer;
}

.header-menu-bar-wrapper .header-menu-bar {
    background-color: #FFF;
    width: 30px;
    height: 5px;
    margin-bottom: 5px;
}

.narcity-header-container {
    display: block;
    margin:auto;
    height: 100%;

    width: {=fw};
}

.narcity-logo-wrapper {
    height: 100%;
    display: inline-block;
    vertical-align: middle;
    margin-right: 15px;
}

.narcity-logo-wrapper img {
    height: 100%;
    width: auto;
    display: block;
}

.awrapper,
#article-content>div.awrapper {
    background-color: #EEE;
    padding: 12px 0px;
    text-align: center;
    border-bottom: 1px solid #CCC;
    border-top: 1px solid #CCC;
    margin-bottom: 5px;
}

.city-selector-wrapper {
    display: inline-block;
    vertical-align: middle;

    margin-left: 20px;
    padding-bottom: 5px;
}

.city-selector-dropper span {
    display: inline-block;
    vertical-align: middle;
}

.city-selector-dropper::after {
    content: "\25BC";
    margin-left: 6px;
    display: inline-block;
    vertical-align: middle;
    font-size: 14px;
}

.city-selector-wrapper a,
.header-link a {
    text-decoration: none;
    color: #FFF;
}

.city-selector {
    display: none;
}

.header-link {
    display: inline-block;
    vertical-align: middle;
    margin: 0px 10px;
    font-size: 18px;
}

.header-float-right {
    float: right;
    height: 100%;
}

.header-login-wrapper {
    height: 100%;
    vertical-align: middle;
    padding-top: 11px;
    display: inline-block;
}

.header-search-wrapper {
    height: 100%;
    vertical-align: middle;
    display: inline-block;
}

.header-search {
    background-color: transparent;
    border: 2px solid #FFF;
    padding: 10px 14px;
    color: #FFF;
    font-size: 16px;
    font-family: Arial;
    margin-top: 8px;
}

.header-search::-webkit-input-placeholder {
    color: #FFF;
    opacity: 1;
}
.header-search:-moz-placeholder {
    color: #FFF;
    opacity: 1;
}
.header-search::-moz-placeholder {
    color: #FFF;
    opacity: 1;
}
.header-search:-ms-input-placeholder {
    color: #FFF;
    opacity: 1;
}


.login-button {
    color: {=theme.settings.buttontextcolor};
    background-color: {=theme.settings.buttoncolor};
    padding: 5px 10px;
    font-size: 18px;
    margin-left: 15px;
    float: left;
    font-family: Oswald, sans-serif;
    text-transform: uppercase;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -ms-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
    border-bottom: 2px solid #AAA;
}

h4.article-section-title {
    font-family: Oswald, sans-serif;
    font-size: 24px;
    font-weight: normal;
    text-transform: uppercase;
    color: {=mc};
    clear: both;
    margin: 30px 0 20px;
    text-align: left;
}

.taboola-wrapper  {
    width: auto;
    margin: 20px 0px;
    display: block;
}

h4.article-section-title a {
    text-decoration: none;
    color: {=mc};
}

#article-section {
    width: {=fw};
    margin: auto;
    display : block;
    margin-top: 30px;
}

.styled-page-container #article-section {
    margin-top: 0;
}

article .article-author-strip {
    width: {=aw};
    margin: 30px 0px;
    font-family: Arial, sans-serif;
}

article .article-author-face-section {
    font-size: 14px;
}

article .article-published-date {
    color: #999;
}

article .article-author-strip .article-author-face {
    width: 50px;
    height: 50px;

    border-radius: 50px;
    margin-right: 6px;
}

article .article-author-share-section {
    float: right;
    color: {=mc};
}

article .article-share-count-wrapper .article-share-count {
    display: block;
    font-size: 30px;
    line-height: 30px;
    text-align: right;
}

article .article-share-count-wrapper .article-share-words {
    display: block;
    font-size: 15px;
    text-align: right;
}

article .article-share-arrow-wrapper {
    background-color: #DDD;

    padding: 10px 6px;
    height: 22px;
    margin-bottom: 10px;
    margin-left: 6px;

    border-radius: 4px;
}

article .article-share-arrow-wrapper .article-social-upward-arrow {
    width: 34px;
}

article #article-content .keep-on-reading-wrapper a.keep-on-reading {
    color: #FFF;
    background-color: #777;
    border-radius: 3px;
    text-align: center;
    padding: 10px;
    cursor: pointer;
    display: block;
    font-family: "Oswald", sans-serif;
    margin-top: 20px;
    font-size: 26px;
    font-weight: 500;
    text-transform: uppercase;
}   

article #article-content .keep-on-reading-wrapper a.keep-on-reading.keep-on-reading-next {
    background-color: {=mc};
    
}

article #article-content .keep-on-reading-wrapper .fa-chevron-right {
    font-size: 28px;
    margin-left: 10px;
}

/* Instagram embed */
.lml-instagram-embed-wrapper {
    position: relative;
    margin: 10px 0px;
}

article #article-content a.lml-instagram-op {
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 2px 10px;
    color: #FFF;
    text-shadow: 0px 0px 5px #000;
    background-color: rgba(0,0,0,0.4);
    font-family: Arial;
}

article #article-content a.lml-instagram-op-2 {
    position: relative;
    bottom: 34px;
    left: 0;
    padding: 0 10px;
    color: #FFF;
    text-shadow: 0 0 5px #000;
    background-color: rgba(0,0,0,0.4);
    font-family: Arial;
    display: block;
    margin-bottom: -32px;
    font-size: 16px;
}

article .featured-image {
    image-rendering: auto;
    object-fit: cover;
    width: 100%;
    height: 100%;
}

article .featured-image-wrapper {
    display: block;
    height: 400px;
    width: {=fw};

    overflow: hidden;
    position: relative;

    text-align: center;
    margin: auto;

    position: relative;
    background-position: center center;
    background-size: cover;
}

.featured-image-credit {
    padding: 8px 20px 0px;
    width: {=fw};
    margin: 0 auto -20px;
    text-align: right;
}

.featured-image-credit a {
    color: #AAA;
    font-weight: bold;
    text-decoration: none;
    font-size: 14px;
}

article #article-content .lml-image-wrapper span,
article #article-content .lml-image-wrapper a {
    color: #AAA;
    font-weight: bold;
    text-decoration: none;
    font-size: 14px;
    position: relative;
    top: -5px;
}

article .social-share-big-count {
    float : right;
    display: none;
}

article .social-share-big-count.shown {
    display: block;
}

article .social-share-big-count-text {
    display: inline-block;
    vertical-align: bottom;
    color: #FF2634;
    text-align: center;
}

article .social-share-big-number {
    font-size: 30px;
    line-height: 30px;
    display: block;
    text-align: right;
}

article .social-share-big-vocab {
    font-size: 18px;
    display: block;
}

article #commentssmallicon {
    vertical-align: middle;
}

article .social-share-big-arrow-wrapper {
    background-color: #DDD;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    border-radius: 4px;
    display: inline-block;
    vertical-align: bottom;

    padding: 10px 6px 0px;
    height: 32px;
    margin-left: 6px;
    margin-bottom: 10px;
}

article .social-share-big-arrow-wrapper img {
    width: 34px;
}

article #article-content {
    width: {=aw};
    margin: 20px 0;
    display: block;
}

article.styled-page-container #article-content.styledpage-content {
    width: {=fw};
    float: none;
}

article .extra-content {
    width: {=aw};
    margin: 0px auto;
    display: block;
}

#article-content {
    float: left;
}

article #article-content,
article #article-content > p,
article #article-content > address,
article #article-content > div,
article .extra-content p,
article .extra-content div {
    font-family: 'PT Serif', serif;
    font-size: 20px;
    line-height: 1.7em;
    margin-bottom: 1.7em;
}

article #article-content h3 {
    font-family: 'PT Serif', serif;
    font-size: 26px;
}

article hr {
    background-color: #D9D9D9;
    height: 1px;
    width: auto;
    display: block;
    border: none;
}

article #article-content a,
article .extra-content a,
article #article-content a:visited {
    color: {=mc};
    text-decoration: none;
}

article > h1 {
    font-size: 40px;
    font-weight: normal;
    margin: 5px auto 15px;
    padding: 0 0 10px;
    font-family: 'Oswald', sans-serif;
    line-height: 1.3em;
    color: #121212;
    line-height: 46px;

    width: {=fw};
}

article.styled-page-container > h1 {
    margin-top: 50px;
    margin-bottom: 0;
}

article > h2 {
    font-size: 18px;
    width: {=fw};
    color : #121212;

    display: block;
    margin: 0 auto 40px;
    border: 0;
    font-family: 'PT Serif', serif;
    font-weight: normal;
}

.title-sponsored-wrapper {
    width: {=fw};
    display: block;
    position: relative;
    margin: auto;
}

.sponsored-mention {
    color: #FFF !important;
    background-color: #ff2634;
    font-size: 15px;
    padding: 3px 5px;
    font-weight: bold;
    letter-spacing: -0.3px;
    border-radius: 4px;
    text-decoration: none;
    font-family: Arial, sans-serif;
}

.archive-image-link {
    display: block;
    width: 100%;
    height: 100%;
}

.sponsored-mention-article{
    position: absolute;
    right: 0px;
    top: 16px;
}

.sponsored-mention-archive, .sponsored-mention-hp{
    position: absolute;
    right: 5px;
    top: 5px;
    border-top-right-radius: 0px;
    z-index: 10px;
}

article #article-content img {
    display: block;

    max-width: 100%;
    width: 100%;
    height: auto;
}

article #article-content img.image-asis {
    display: none;
}

article .article-signature {
    width: {=awpad};
    margin: 20px auto;

    padding: 20px;
    border: 2px solid #EEE;
}

article .article-signature-face-wrapper {
    width: 15%;
    display: inline-block;
    margin-right: 15px;

    vertical-align: top;
}

article .article-signature-details-wrapper {
    width: 80%;
    display: inline-block;
    margin-top: -5px;

    vertical-align: top;
    font-family: Arial;
}

article .article-signature-face {
    width: 90px;
    height: 90px;

    border-radius: 90px;
}

article .article-signature-name a,
article .article-signature-name a:visited {
    display: inline;
    color: {=mc};
    font-size: 18px;
    font-weight: bold;
    text-align: left;
}

article .article-signature-name a {
    text-decoration: none;
}

article .article-signature-bio {
    display: block;
    color: #999;
    font-family: Arial,sans-serif;
    margin-top: 6px;
    text-align: justify;
    line-height: 1.7;
    font-size: 14px;
}

article #article-content .article-signature-bio h3 {
    font-size: 15px;
    margin-top: -12px;
    padding-top: 0;
    font-family: Arial;
    display: block;
    color: #999;
    font-weight: 700;
}

article .article-more-from-wrapper {
    width: {=aw};
    margin: 10px auto;
}

article .article-more-from-list {
    list-style: none;
    margin: 0;
    padding: 0;
}

article .article-more-from-list li {
    list-style: none;
    margin: 0;
    padding: 10px 0px;
    border-bottom: 1px solid #DDD;
    line-height: 22px;
}

article .article-more-from-list li a,
article .article-more-from-list li a {
    color: {=mc};
    text-decoration: none;
}

article .article-more-from-list li:last-child {
    border-bottom: none;
}

article .article-tags-wrapper {
    width: auto;
    margin: 40px 0px;
}

article .article-tags-wrapper a {
    display: inline-block;
    white-space: nowrap;
    padding: 0px 12px 0px 12px;
    background-color: #FFF;
    color: #000;
    margin: 0px 8px 5px 0px;
    font-size: 15px;
    text-transform: capitalize;
    line-height: 32px;
    position: relative;
    text-decoration: none;
    font-family: Arial, sans-serif;
}

article .article-tags-wrapper a::before {
    display: block;
    margin-right: 4px;
    background-color: {=mc};
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    width: 3px;
    content: " ";
}

article .article-comments {
    width: {=aw};
    margin: 20px auto;
}

footer .footer-digest-sub {
    width: {=aw};
}

footer .footer-digest-sub form {
    font-family: "Oswald",sans-serif;
    text-transform: uppercase;
    color: white;
    font-size: 23px;
    display: block;
}

footer .footer-digest-sub form input {
    display: inline-block;
}

footer .footer-digest-sub .events-placeholder__submit-button {
    padding: 12px 15px 9px 15px;
    margin-left: 20px;
    background-color: {=mc};
    cursor: pointer;
    -webkit-tap-highlight-color: transparent;
    color: #FFF;
    border: none;
    font-size: 18px;
    overflow:hidden;
}

footer .footer-digest-sub .events-placeholder__email-input {
    padding: 7px;
    font-size: 1em;
    border: 1px solid #ddd;
    background-color: #FFF;
    color: #888;
    margin-right: -4px;
    float:left;
}

article .article-related-section {
    width: {=aw};
    margin: 20px auto;
    text-align: center;
}

article .article-related-section a {
    color: #111;
    text-decoration: none;
}

article .article-related-section .article-thumbnail {
    border: 1px solid #D9D9D9;
    width: 49%;
    display: inline-block;
    vertical-align: top;
    margin: 4px 0px;
}

article .article-related-section .article-thumbnail-featured-image img {
    width: 100%;
    position: relative;
}

article .article-related-section .article-thumbnail-title {
    font-family: "Oswald", Arial, sans-serif;
    font-size: 20px;
    padding: 5px 10px;
    text-align: left;
}

article .article-related-section .article-thumbnail-featured {
    width: auto;
    display: block;
}

article .article-related-section .article-thumbnail-featured .article-thumbnail-title {
    font-size: 38px;
}

.sidebar-internal-link-image {
    height: 160px;
    width: 300px;
    object-fit: cover;
    object-position: center center;
}

.sidebar-internal-link-title {
    font-size: 20px;
    color: #111;
    font-family: "Oswald",sans-serif;
    text-decoration: none;
    display: block;
    margin: 5px 0 0;
}

.sidebar-internal-link-subtitle {
    display: none;
    color: #333;
    margin-top: 1px;
    font-size: 16px;
}

.sidebar-internal-link {
    text-decoration: none;
    margin-top: 40px;
    display: block;
}

#sidebar-links h3,
article #article-content #article-bottom-links h3 {
    font-family: Oswald,sans-serif;
    font-size: 24px;
    font-weight: normal;
    text-transform: uppercase;
    color: #ff2634 ;
    clear: both;
    margin: 30px 0 20px;
    text-align: left;
}

article #article-content #article-bottom-links h3 {
    padding: 0px;
}

.fwblock {
    width: {=fw};
    margin: auto;
    float: none;
    clear: both;
}

.clear {
    clear : both;
    float : none;
}

.banner-wrapper {
    text-align: center;
    padding: 10px 0px;
}

lml\:template, lml\:native {
    display: none;
}

/* Homepage */
{$
    hw = theme.settings.fullwidthsize;
