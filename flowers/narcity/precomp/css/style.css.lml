}
.adblock-nativead-link__title a {
  color: #111;
}
.adblock-nativead-link__title h2 {
  font-family:Oswald, sans-serif;
  font-size: 22px;
  padding: 10px 10px 0;
  line-height: 1.4;
}
.adblock-nativead-link__subtitle {
  font-size: 12px;
