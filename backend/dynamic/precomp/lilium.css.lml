#post_create select,
#post_create textarea,
#post_create input[type="date"],
#post_create input[type="number"],
#post_create input[type="text"] {
    width: 640px;
}

.btn::before {
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;    
    margin-right: 8px;
}

.btn-save::before {
    content: '\f0c6';
}

.btn-preview::before {
    content: '\f06e';
}

.btn-publish::before {
    content: '\f1d9';
}

.announcement.modal .modal-body {
    position: relative; 
    padding: 15px; 
    text-align: center;
    font-size: 26px;
    color: #777;
}

.announcement.modal .modal-body::before {
    content: 'fd';
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    font-size: 200px;
    color: #EEE;
    font-weight: normal;
    margin: 30px;
}

#confirm-publish .modal-body::before {
    content: '\f1d9';
    -webkit-animation: slowfloat 8s ease-in-out 2s infinite;
    -moz-animation: slowfloat 8s ease-in-out 2s infinite;
    animation: slowfloat 8s ease-in-out 0s infinite;
}

#confirm-unpublish .modal-body::before {
    content: '\f04a';
    -webkit-animation: rew 4s linear 2s infinite;
    -moz-animation: rew 4s linear 2s infinite;
    animation: rew 4s linear 0s infinite;
}

#confirm-destroy .modal-body::before {
    content: '\f0f6';
    -webkit-animation: drop 4s linear 2s infinite;
    -moz-animation: drop 4s linear 2s infinite;
    animation: drop 4s linear 0s infinite;
}

@-webkit-keyframes slowfloat {
    0%   { -webkit-transform: translate3d(0, 0px,  0); }
    50%  { -webkit-transform: translate3d(0, -30px, 0); }
    100% { -webkit-transform: translate3d(0, 0px,  0); }
}

@-moz-keyframes slowfloat {
    0%   { -moz-transform: translate3d(0, 0px,  0); }
    50%  { -moz-transform: translate3d(0, -30px, 0); }
    100% { -moz-transform: translate3d(0, 0px,  0); }
}   

@keyframes slowfloat {
    0%   { transform: translate3d(0, 0px,  0); }
    50%  { transform: translate3d(0, -30px, 0); }
    100% { transform: translate3d(0, 0px,  0); }
}

@keyframes rew {
    0%   { transform: translate3d(0px,   0px, 0px); opacity: 1;   }
    50%  { transform: translate3d(-30px, 0px, 0px); opacity: 0; }
    51%  { transform: translate3d(30px, 0px, 0px); opacity: 0; }
    100% { transform: translate3d(0px,   0px, 0px); opacity: 1;   }
}   

@keyframes drop {
    0%   { transform: translate3d(0px,   0px, 0px); opacity: 1;   }
    10%   { transform: translate3d(0px,   0px, 0px); opacity: 1;   }
    80%  { transform: translate3d(0px,  30px, 0px); opacity: 0; }
    81%  { transform: translate3d(0px,   0px, 0px); opacity: 0; }
    100% { transform: translate3d(0px,   0px, 0px); opacity: 1;   }
}   
