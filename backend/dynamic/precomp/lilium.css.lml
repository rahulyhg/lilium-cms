html, body {
	padding:0;
	margin:0;

	font-family: 'Raleway', sans-serif;
	color: {=font-color};
}

body {
	opacity:1;
	-webkit-transition: opacity 0.2s;
   	   -moz-transition: opacity 0.2s;
	    -ms-transition: opacity 0.2s;
	     -o-transition: opacity 0.2s;
		transition: opacity 0.2s;
}

body.lmlrendered {
	opacity: 1;
}

input, textarea, button {
	outline: none;
	border: none;
}

a {
	color: {=actioncolor};
}

a:hover {
	color: {=activecolor};
}

button, input[type="submit"] {
	cursor: pointer;
}

input[type="checkbox"], .lmlcheckbox {
	display: none;
}

.uppercase {
    text-transform: uppercase;
}

.lmlbeautifiedcheckbox {
	display: inline-block;
	width: 32px;
	height:32px;
	vertical-align: middle;

    font: normal normal normal 14px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;

	margin-right: 10px;
	cursor: pointer;

    font-size: 26px;
    text-align: center;
    padding-top: 2px;

    border: 1px dashed #CCC;
    border-radius: 5px;
}


.lmlbeautifiedcheckbox::before {
    color: #E84C3D;
    content: "\f00d";
}

.lmlbeautifiedcheckbox.checked::before {
    content: "\f00c";
    color : #2ECD71;
}

form .campaigncreatefield-checkbox label {
	display: inline-block;
}

lml\:template, lml\:tobject, lml,
lml\:queryparam, .lmlloading, .lmlhidden {
	display: none;
}

h1, h2, h3, h4, h5, h6 {
	margin:0;
	padding:0;
	font-weight: 100;
}

form input, form label{
	display: block;
}
select {
    color: #666;
    background: #fff;
}
.lilium {
	padding:0;
	margin:0;
}

/* Lilium */
/* Bootstrap */
.container {
    padding-left: 200px;
    width: 100%;
    padding-right: 0px;
}
.row{
    margin-right: 0px;
    margin-left: 0px;
}

/* Normalized Forms */
.lmlform {
	background-color: #F6F6F6;
	border-top: 1px solid #DDD;
	border-bottom: 1px solid #DDD;
}

.lmlform select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance : none;

    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;

    
}

.select-wrapper:before {
    content: '\f0d7';
    font-family: FontAwesome;
    color: #fff;
    display: inline-block;
    position: absolute;
    right: 20px;
    top: 15px;
    pointer-events: none;
}

.lmlform select::-ms-expand {
    display: none;
}

.lmlform-fieldwrapper {
	padding: 10px;
}

.lmlform-fieldwrapper-title {
    clear: both;
    float: none;
    display: block;
}

.lmlform-fieldwrapper-hidden {
    display: none;
}

.lmlform-fieldwrapper label {
	margin-right: 10px;
}

.lmlform-fieldwrapper label.inline, .lmlform input.inline,
.lmlform select.inline, .lmlform textarea.inline {
	display: inline-block;
}

.lmlform input:not([type="file"]), .lmlform select, .lmlform textarea {
	padding: 10px;
	font-size: 14px;
	border: 1px solid #DFDFDF;
	width: 100%;
    box-sizing: border-box;
}

.lmlform .ck-image-credit-field-text input {
    background-color: #FFF;
}

.cke_dialog #image-upload h1 {
    font-size: 18px;
    font-family: Open Sans, sans-serif;
    display: block;
    margin-bottom: 10px;
}

.cke_dialog .picture-list-form,
.cke_dialog .picture-upload-form {
    margin: 0px -10px -10px -10px;
}

.lmlform input[type="submit"], .lmlform button {
    width: auto;
    margin: 10px;

    border-radius: 6px;
    padding: 6px 10px;

    font-size: 14px;
	border: none;

	background-color: {=actioncolor};
	color: #EEE;
    
    font-family: Arial, sans-serif;
}

.lmlform input[type="submit"]:hover, .lmlform button:hover {
	background-color: {=hovercolor};
    color: #EEE;
}

.lmlform input[type="submit"]:active, .lmlform button:active {
	background-color: {=activecolor};
    color: #EEE;
}

.lmlform input:focus, .lmlform select:focus,
.lmlform textarea:focus {
	border: 1px solid {=actioncolor};
}

.lmlform .lmlform-fieldwrapper-title {
	background-color: #EEE;
}

.lmlform .lmlform-fieldwrapper-title:not(:first-child) {
	border-top: 1px solid #DDD;
}

.lmlform .lmlform-fieldwrapper-title:not(:last-child) {
	border-bottom: 1px solid #DDD;
}

.lmlform .lmlform-fieldwrapper-title h3 {
	text-shadow: 0 0 3px rgba(0,0,0,0.1);
}

.lml-button {
	background-color: {=actioncolor};
	color: #EEE;
	font-size: 18px;
    padding: 10px;
	border: none;
}

.lmladmincontentwrapper {
	padding-left: {=sidebarwidth};
	padding-top: {=topbarheight};
}

.lmladminsidebardecowrapper {
    overflow: hidden;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    top: 0;
    z-index: 1000;
}

.lmladminsidebardeco {
	display: block;
    	background-color: #000;
    	position: absolute;
    	bottom: 0;
    	height: 600px;
    	left: 0;
    	right: 0;
    	opacity: 0.05;
}

.lmladminbardisplayname {
    width: 130px;
    text-overflow: ellipsis;
    overflow: hidden;    
}

.lmladminsidebardeco:nth-child(1) {
	-webkit-transform: translateX(79px) translateY(86px) rotateZ(25deg);
	-moz-transform: translateX(79px) translateY(86px) rotateZ(25deg);
	-ms-transform: translateX(79px) translateY(86px) rotateZ(25deg);
	-o-transform: translateX(79px) translateY(86px) rotateZ(25deg);
	transform: translateX(79px) translateY(86px) rotateZ(25deg);
}

.lmladminsidebardeco:nth-child(2) {
    	-webkit-transform: translateX(103px) translateY(126px) rotateZ(56deg);
    	-moz-transform: translateX(103px) translateY(126px) rotateZ(56deg);
    	-ms-transform: translateX(103px) translateY(126px) rotateZ(56deg);
    	-o-transform: translateX(103px) translateY(126px) rotateZ(56deg);
    	transform: translateX(103px) translateY(126px) rotateZ(56deg);
}

.lmladminsidebardeco:nth-child(3) {
    	-webkit-transform: translateX(100px) translateY(172px) rotateZ(94deg);
    	-moz-transform: translateX(100px) translateY(172px) rotateZ(94deg);
    	-ms-transform: translateX(100px) translateY(172px) rotateZ(94deg);
    	-o-transform: translateX(100px) translateY(172px) rotateZ(94deg);
    	transform: translateX(100px) translateY(172px) rotateZ(94deg);
}

/* Admin Body */
main.lmladminmain {

}

main.lmladminmain h1 {
    	padding: 10px;
    	background-color: #EEE;
    	color: #777;
    	font-size: 36px;
    	border-bottom: 1px solid #DDD;
    	text-shadow: 0px 0px 3px rgba(0, 0, 0, 0.1);
}

main.lmladminmain .pad10 {
	padding: 10px 15px;
	display: block;
}

h2.subtitlestrip {
	padding: 10px;
    	border-bottom: 1px solid #EEE;
    	border-top: 1px solid #eee;
    	background-color: #fafafa;
    	color: #999;
}

/* Admin Sidebar */
.lmladminsidebar {
	position: fixed;
	left: 0;
	bottom: 0;
	top: {=topbarheight};

	width: {=sidebarwidth};
	background-color: {=barcolor};

	overflow-x: hidden;
	z-index: 20000;
}

.lmladminsidebar .lmladminsidebaritems {
	padding:0;
	margin: 0;

	color: #BBB;
}

.lmladminsidebar .lmladminsidebaritems li {
	cursor: pointer;
	font-size: 14px;
}

.lmladminsidebar .lmladminsidebaritems li.lmladminsidebarselected,
.lmladminsidebar .lmladminsidebaritems li.lmladminsidebarselected:hover {
	background-color: {=actioncolor};
}

.lmladminsidebar .lmladminsidebaritems li.lmladminsidebarselected a,
.lmladminsidebar .lmladminsidebaritems li.lmladminsidebarselected:hover a {
	color: #FFF;
}

.lmladminsidebar .lmladminsidebaritems li:hover {
	background-color: {=barcolorhover};
	color: #EEE;
}

.lmladminsidebar .lmladminsidebaritems li a {
	display: block;
	padding: 9px 12px;
	color: #BBB;

	text-decoration: none;
}

.lmladminsidebar .lmladminsidebaritems li:hover a {
	color: #EEE;
}

.lmladminsidebar .lmladminsidebaritems .fa {
	font-size: 18px;
	margin-right: 5px;

	text-decoration: none;
	width: 22px;
}

.fa > .badge{
	position: relative;
	top: -10px;
	background-color: {=actioncolor};
	border-radius: 2px;
	line-height: 15px;
}

.lmladminsidebarlistwrapper {
	position:relative;
	z-index: 2000;
}
.lmladminsidebar .lmladminsidebaritems .parent > a > i:nth-of-type(2){
    float: right;
    margin: 0px;
    width: 8px;

    -webkit-transition: all 300ms cubic-bezier(0.740, 0, 0.280, 1.000); /* older webkit */
    -webkit-transition: all 300ms cubic-bezier(0.740, -0.005, 0.280, 1.000);
       -moz-transition: all 300ms cubic-bezier(0.740, -0.005, 0.280, 1.000);
         -o-transition: all 300ms cubic-bezier(0.740, -0.005, 0.280, 1.000);
            transition: all 300ms cubic-bezier(0.740, -0.005, 0.280, 1.000); /* custom */

    -webkit-transition-timing-function: cubic-bezier(0.740, 0, 0.280, 1.000); /* older webkit */
    -webkit-transition-timing-function: cubic-bezier(0.740, -0.005, 0.280, 1.000);
       -moz-transition-timing-function: cubic-bezier(0.740, -0.005, 0.280, 1.000);
         -o-transition-timing-function: cubic-bezier(0.740, -0.005, 0.280, 1.000);
            transition-timing-function: cubic-bezier(0.740, -0.005, 0.280, 1.000); /* custom */
}
.lmladminsidebar .lmladminsidebaritems .parent:hover > a > i:nth-of-type(2){
    -webkit-transform : rotate(-90deg);
    -moz-transform : rotate(-90deg);
    -ms-transform: rotate(-90deg);
    transform: rotate(-90deg);
}
.lmladminsidebar .lmladminsidebaritems .parent:hover ul{
	max-height: 100%;
	height: 100%;

}

.lmladminsidebar .lmladminsidebaritems .parent ul {
	overflow: hidden;
	border-left : 5px solid {=activecolor};


}
.lmladminsidebar .lmladminsidebaritems .parent ul li {
	-webkit-transition: height 0.3s;
	transition: height 0.3s;
	transition-timing-function: ease;
	height: 0px;
}
.lmladminsidebar .lmladminsidebaritems .parent:hover ul li {
	height: 38px;
}
.lmladminsidebar .lmladminsidebaritems .parent.child-selected ul li {
	height: 38px;
}
.lmladminsidebar .lmladminsidebaritems .parent.child-selected > a > i:nth-of-type(2){
    -webkit-transform : rotate(-90deg);
    -moz-transform : rotate(-90deg);
