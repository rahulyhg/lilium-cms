{#config;forms}
<!DOCTYPE html>
<html>
	<head>
		{%globalhead}
	</head>
	<body>
		<h1>All Campaigns</h1>
		{*campaigns.all(template:"single-campaign")}

		<h2>Create Campaign</h2>
		{=forms.render('campaign_create')}

		<lml:template id="single-campaign">
            <li class="entity-item-list">
                <lml:tobject data-nodetype="h3" data-key="displayname"></lml:tobject>
            </li>
		</lml:template>
	</body>
</html>
