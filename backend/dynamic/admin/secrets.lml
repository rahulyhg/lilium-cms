{#config;extra}
<h1>Secrets</h1>

<div class="lmlformattedform">
    {$ for (secsec in ?extra.secrets); $}
        <h2>{=secsec.groupname}</h2>
        {$ for (sec in ?secsec.secrets); $}
            <div>
                <a href="#" class="secret-link" data-name="{=sec.name}">{=sec.displayname}</a>
            </div>
        {$ endfor; $}
        <div><a href="#" data-name="" class="secret-link secret-add-new">Add new secret</a></div>
    {$ endfor; $}
</div>

<button>Add new section</button>
