{#themes;vocab;debug}
{$
	themeList = themes.getCachedThemes();
$}
<!DOCTYPE html>
<html>

<head>
	<title>{=vocab.themes}</title>
</head>

<body>
	<h1>{=vocab.themes}</h1>

	{$
		for (theme in themeList);
	 $}
	 	{*theme.{=theme.uName}.active}
	<h2>{=theme.dName}</h2>
	<div class="themes-actions">
		<button>Enable</button>

		<button>Disable</button>

	</div>
	{$
		endfor;
	$}
</body>

</html>
