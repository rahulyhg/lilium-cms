{#config}
<script>
	var deleteUser = function(id) {
		$.post('{=config.default.server.url}/admin/entities', {
			"uid" : id,
			"form_name" : "entity_delete"
		}, function(data) {
			liliumcms.refresh();
		});
	};
	
	var canDelete = function(item) {
		return typeof item.id === 'undefined' || item.id > 1;
	};
</script>
