<!DOCTYPE html>
<html>
	<head>
		{%globalhead}
	</head>
	<body>
		<h1>All entities</h1>

		<main class="entities-main"> 
			<ul class="entity-list">
				{*entities(template:"entity-list-item")}
			</ul>
		</main>

		<lml:template id="entity-list-item">
			<li class="entity-item-list">
				<lml:tobject data-nodetype="h2" data-key="displayname"></lml:tobject>
				<div class="entity-actions">
					<button>delete</button>
				</div>
			</li>
		</lml:template>
	</body>
</html>
