{#extra}
<!DOCTYPE html>
<html>
	<head>
		{%adminhead}
	</head>
	<body class="errorbody lmlreport lmlcrash">
		<header>
			<h1>Lilium crash report</h1>
		</header>
		<main>
			<h2>{=extra.message}</h2>
			<div class="stacktrace">
				<pre>
					{=extra.stack}
				</pre>
			</div>
		</main>
	</body>
</html>
